{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Dev server",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
              "run",
              "dev"
            ],
            "type": "node",
          },
        {
            "name": "Launch Firefox",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:5173",
        }
    ]
}